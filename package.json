    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cuid": "^3.0.0",
    "json-schema-to-typescript": "^15.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.414.0",
    "mime-types": "^2.1.35",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "nodemailer": "^6.9.14",
    "react": "^18",
