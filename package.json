{
  "name": "next-test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier --write .",
    "server": "json-server --watch db.json --port 9999",
    "prisma": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset",
    "docker": "docker compose up -d",
    "test": "jest",
    "e2e": "npx playwright test",
    "test-gen": "npx playwright codegen",
    "test:e2e": "npx prisma migrate reset --force && playwright test",
    "update-content-schemas": "ts-node --project tsconfig.script.json ./scripts/upload-content-schema https://raw.githubusercontent.com/rusyander/content-schema/main/schemas/ ./src/shared/api/content/schemas"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.script.json prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
